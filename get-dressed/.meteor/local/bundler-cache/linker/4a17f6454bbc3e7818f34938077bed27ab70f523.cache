ect-outfit');\n});\n\nRouter.route('/outfits', function () {\n  this.render('outfits');\n});\n\nRouter.route('/login', {\n  name: 'login',\n  template: 'login',\n\n  action: function() {\n    if ( Meteor.userId() != null ) {\n      Router.go('/dashboard');\n    }\n    else {\n      this.render();\n    }\n  }\n});\n\nRouter.route('/register', function () {\n  name: 'register',\n  this.render('register');\n});\n\nRouter.route('/add-item-tag', function () {\n  this.render('add-item-tag');\n});\n\nRouter.route('/', {\n  template: 'login'\n});\n\n"]}}]