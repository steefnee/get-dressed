H,MAAM,CAAC,KAAK,CAAC,QAAQ,EAAE;AACrB,MAAI,EAAE,OAAO;AACb,UAAQ,EAAE,OAAO;;AAEjB,QAAM,EAAE,YAAW;AACjB,QAAK,MAAM,CAAC,MAAM,EAAE,IAAI,IAAI,EAAG;AAC7B,YAAM,CAAC,EAAE,CAAC,YAAY,CAAC,CAAC;KACzB,MACI;AACH,UAAI,CAAC,MAAM,EAAE,CAAC;KACf;GACF;CACF,CAAC,CAAC;;AAEH,MAAM,CAAC,KAAK,CAAC,WAAW,EAAE,YAAY;AACpC,MAAI,EAAE,UAAU,EAChB,IAAI,CAAC,MAAM,CAAC,UAAU,CAAC,CAAC;CACzB,CAAC,CAAC;;AAEH,MAAM,CAAC,KAAK,CAAC,eAAe,EAAE,YAAY;AACxC,MAAI,CAAC,MAAM,CAAC,cAAc,CAAC,CAAC;CAC7B,CAAC,CAAC;;AAEH,MAAM,CAAC,KAAK,CAAC,GAAG,EAAE;AAChB,UAAQ,EAAE,OAAO;CAClB,CAAC,CAAC;;AAEH,MAAM,CAAC,KAAK,CAAC,WAAW,EAAE,YAAY;AACpC,MAAI,EAAE,UAAU,EAChB,IAAI,CAAC,MAAM,CAAC,UAAU,CAAC,CAAC;CACzB,CAAC,CAAC,sE","file":"/scripts/index.js","sourcesContent":["if(Meteor.isClient){\n    Template.register.events({\n      'submit form': function(event) {\n        Router.go('/login');\n        event.preventDefault();\n        var nameVar = event.target.registerName.value;\n        var emailVar = event.target.registerEmail.value;\n        var passwordVar = event.target.registerPassword.value;\n        Accounts.createUser({\n          name: nameVar,\n          email: emailVar,\n          password: passwordVar\n\n        });\n      }\n    });\n\n    Template.login.events({\n      'submit form': function(event) {\n        event.preventDefault();\n        var emailVar = $('#email').val();\n        var passwordVar = $('#password').val();\n        Meteor.loginWithPassword(emailVar, passwordVar, function(error) {\n          if ( error == null ) {\n            Router.go('/dashboard');\n          }\n        });\n      }\n    });\n\n    Template.dashboard.events({\n      'click .logout': function(event){\n          event.preventDefault();\n          Meteor.logout();\n      }\n    });\n}\n\nif(Meteor.isServer){\n    // server code goes here\n}\n\n\nRouter.onBeforeAction(function () {\n  // all properties available in the route function\n  // are also available here such as this.params\n  if (!Meteor.userId()) {\n    // if the user is not logged in, render the Login template\n    this.render('Login');\n  } else {\n    // otherwise don't hold up the rest of hooks or our route/action function\n    // from running\n    this.next();\n  }\n} , {except:['register']});\n\n\nRouter.route('/dashboard', {\n  template: 'dashboard',\n  action: function() {\n    if ( Meteor.userId() == null ) {\n      Router.go('/login');\n    }\n    else {\n      this.render();\n    }\n  }\n});\n\nRouter.route('/occasion-choices', function () {\n  this.render('occasion-choices');\n});\n\nRouter.route('/select-outfit', function () {\n  this.render('select-outfit');\n});\n\nRouter.route('/outfits', function () {\n  this.render('outfits');\n});\n\nRouter.route('/login', {\n  name: 'login',\n  template: 'login',\n\n  action: function() {\n    if ( Meteor.userId() != null ) {\n      Router.go('/dashboard');\n    }\n    else {\n      this.render();\n    }\n  }\n});\n\nRouter.route('/register', function () {\n  name: 'register',\n  this.render('register');\n});\n\nRouter.route('/add-item-tag', function () {\n  this.render('add-item-tag');\n});\n\nRouter.route('/', {\n  template: 'login'\n});\n\nRouter.route('/add-item', function () {\n  name: 'add-item',\n  this.render('add-item');\n});\n\n"]}}]